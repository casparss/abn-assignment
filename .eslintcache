bn-assignment/src/components/Carousel/hooks/useCarousel.ts",["186"],"import { useRef, useState, CSSProperties, useEffect } from \"react\";\n\nconst PAGE_SIZE = 6;\n\nexport const useCarousel = (items: unknown[]) => {\n    const sliderRef = useRef<HTMLDivElement>(null);\n    const [idx, setIdx] = useState<number>(0);\n    const [sliderStyle, setSlideStyle] = useState<CSSProperties>({});\n    const isSliderAtBeginning = idx === 0;\n    const isSliderAtEnd = idx >= items.length - PAGE_SIZE;\n\n    useEffect(() => {\n        moveSlider();\n    }, [idx]);\n\n    const incremetIdx = () => {\n        setIdx(idx + PAGE_SIZE);\n    };\n\n    const decrementIdx = () => {\n        setIdx(idx - PAGE_SIZE);\n    };\n\n    const moveSlider = () => {\n        if (!sliderRef.current) {\n            return;\n        }\n        const width = sliderRef.current.offsetWidth / items.length;\n        setSlideStyle({\n            transform: `translateX(${-(width * idx)}px)`,\n            transition: `transform 400ms ease`,\n        });\n    };\n\n    return {\n        sliderRef,\n        sliderStyle,\n        isSliderAtBeginning,\n        isSliderAtEnd,\n        incremetIdx,\n        decrementIdx,\n    };\n}\n\nexport default useCarousel;","/Users/caswork/Projects/abn-assignment/src/components/HeaderBar/styles.ts",[],"/Users/caswork/Projects/abn-assignment/src/components/Rating/Rating.tsx",[],"/Users/caswork/Projects/abn-assignment/src/components/Rating/styles.ts",[],"/Users/caswork/Projects/abn-assignment/src/components/Rating/index.tsx",[],"/Users/caswork/Projects/abn-assignment/src/components/ShowDetails/styles.ts",[],"/Users/caswork/Projects/abn-assignment/src/store/ShowModel.ts",[],"/Users/caswork/Projects/abn-assignment/src/components/ShowDetails/Info.tsx",["187","188"],"/Users/caswork/Projects/abn-assignment/src/components/ShowDetails/Cover.tsx",["189"],"/Users/caswork/Projects/abn-assignment/src/components/ShowDetails/Seasons.tsx",[],"/Users/caswork/Projects/abn-assignment/src/comp